{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2022",
      "lib": ["ES2022", "WebWorker"],
      "types": ["@cloudflare/workers-types"],
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "allowJs": true,
      "checkJs": false,
      "noEmit": true,
      "isolatedModules": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "src/**/*.js"],
    "exclude": ["node_modules"]
  }